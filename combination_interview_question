//Write a code that will print all the combinations of strings from a given string and a given length? (you can only write a method with two parameter and your solution should use recursive function)

input 
ABCD
3

output
A B C
A B D
A C D
B C D



#include <iostream>
#include <string>

using namespace std;

int dynamic_swap(string s,int n){
    string temp = s;
    if( n >= 0){
        cout<<temp.erase(n,1)<<endl;
        dynamic_swap(s,n-1);
    }
}

int main()
{
    string s = "ABCD";
    dynamic_swap(s,3);

    return 0;
}
